*
    padding: 0
    margin: 0
html, body
    font-size: 14px
li
    list-style: none

a
    text-decoration: none
    color: #ffffff
    opacity: 0.6
img
    vertical-align: middle
.container
    width: 100%
    height: 100%
    overflow-y: auto
    .header
        position: fixed
        top: 0
        left: 0
        width: calc(100% - 287px)
        height: 80px
        line-height: 80px
        padding: 0 135px
        background-color: rgba(#4C4592, .7)
        .h-left
            float: left
            color: #ffffff
            font-size: 18px
            img
                margin-right: 18px
        .h-right
            float: right
            li
                float: left
                margin-left: 60px
                font-size: 15px
                &:first-child
                    margin-left: 0
                a.active
                    opacity: 1
    .main
        #intro
            background-color: #4C4592
            overflow: hidden
            padding: 175 135px 100px
            .intro-left
                width: 340px
                height: 434px
                float: left
            .intro-right
                float: left
                margin-left: 100px
                width: calc(100% - 440px)
                .intro-text
                    color: rgba(#ffffff, .8)
                    line-height: 26px
                .download
                    margin-top: 45px
                    width: 160px
                    height: 50px
                    line-height: 50px
                    text-align: center
                    border-radius: 25px
                    background:linear-gradient(90deg,rgba(250,217,97,1) 0%,rgba(247,107,28,1) 100%)
                    color: #ffffff
                    cursor: pointer
            .module-title
                color: #ffffff
                text-align: left
                &:after
                    content: ''
                    position: absolute
                    width: 40px
                    height: 5px
                    bottom: 0
                    left: 0
                    margin-left: 0
                    background-color: #FFB400
            .sub-title
                color: rgba(#ffffff, .5)
                margin: 20px 0
                text-align: left
        #func-module
            padding: 100px 135px
            overflow: hidden
            background-color: #ffffff
            .func-list
                margin-top: 80px
                display: flex
                .list-item
                    flex: 1
                    height: 251px
                    box-shadow:0px 4px 26px 0px rgba(0,0,0,0.08)
                    border-radius: 10px
                    margin-right: 30px
                    padding: 55px 25px 70px
                    text-align: center
                    &:last-child
                        margin-right: 0
                    .item-img
                        width: 118px
                        height: 118px
                        line-height: 118px
                        border-radius: 50%
                        background-color: #F2F2F2
                        margin: 0 auto
                        img
                            margin-top: 40px
                    .item-title
                        margin: 35px 0 20px
                        font-size: 24px
                        color: #424345
                    .item-examples
                        color: #838383
        #economic-model
            background-color: #4C4592
            overflow: hidden
            padding: 100px 135px
            .module-title
                color: #ffffff
            .sub-title
                color: rgba(#ffffff, .5)
            .model-list
                margin-top: 90px
                overflow: hidden
                .list-item
                    margin-bottom: 90px
                    align-items: center
                    width: 30%
                    margin-right: 5%
                    float: left
                    &:nth-child(3), &:nth-child(6)
                        margin-right: 0
                    .title
                        color: #ffffff
                        font-size: 16px
                        margin-bottom: 23px
                        img
                            width: 32px
                            height: 32px
                            margin-right: 16px
                    p
                        color: rgba(#ffffff, .5)
                        line-height: 27px
        #team
            background-color: #ffffff
            overflow: hidden
            padding: 100px 135px
            .team-list
                margin-top: 100px
                .list-item
                    margin-bottom: 30px
                    width: 48%
                    margin-right: 4%
                    box-shadow: 0px 4px 26px 0px rgba(0,0,0,0.1)
                    float: left
                    &:nth-child(2), &:nth-child(4)
                        margin-right: 0
                    .item-left
                        float: left
                        width: 118px
                        height: 118px
                        border-radius: 50%
                        margin: 50px 16px 35px
                        img
                            border-radius: 50%
                    .item-right
                        float: left
                        width: calc(100% - 170px)
                        margin: 50px 20px 20px 0
                        .title
                            color: #424345
                            font-size: 24px
                            margin-bottom: 14px
                            span
                                font-size: 14px
                                color: #A5A5A5
                                margin-left: 20px
                        p
                            color: #838383
                            line-height: 22px
                            overflow:hidden
                            text-overflow:ellipsis
                            display:-webkit-box
                            -webkit-box-orient:vertical
                            -webkit-line-clamp:4  
        #about
            background-color: #4C4592
            overflow: hidden
            padding: 100px 135px
            text-align: center
            .module-title
                color: #ffffff
            .sub-title
                color: rgba(#ffffff, .5)
                margin-bottom: 100px
            img
                max-width: 100%
        .module-title
            font-size: 39px
            color: #424345
            position: relative
            height: 90px
            line-height: 90px
            text-align: center
            &:after
                content: ''
                position: absolute
                width: 40px
                height: 5px
                bottom: 0
                left: 50%
                margin-left: -20px
                background-color: #FFB400
        .sub-title
            color: #838383
            margin: 25px 0
            text-align: center
    .footer
        height: 100px
        line-height: 100px
        padding 0 135px
        background-color: #ffffff
        .f-left
            float: left
            color: #424345
            font-size: 18px
            img
                margin-right: 18px
        .f-right
            float: right
            font-size: 16px
            color: #838383

@media screen and (max-width:768px)
    .container
        .header
            position: static
            width: 100%
            height: 100px
            line-height: 100px
            background-color: #4C4592
            padding: 0
            .h-left
                float: unset
                text-align: center
            .h-right
                display: none   
        .main
            text-align: center
            #intro
                background-color: #4C4592
                overflow: hidden
                padding: 25px
                .intro-left
                    display: none
                .intro-right
                    float: unset
                    margin-left: 0
                    width: 100%
                    .intro-text
                        color: rgba(#ffffff, .8)
                        line-height: 24px
                        text-align: justify
                    .download
                        width: 100px
                        height: 36px
                        line-height: 36px
                        text-align: center
                        border-radius: 25px
                        background:linear-gradient(90deg,rgba(250,217,97,1) 0%,rgba(247,107,28,1) 100%)
                        color: #ffffff
                        cursor: pointer
                        margin: 25px auto 0
                    .module-title
                        color: #ffffff
                        text-align: center
                        &:after
                            content: ''
                            position: absolute
                            width: 26px
                            height: 3px
                            bottom: 0
                            left: 50%
                            margin-left: -13px
                            background-color: #FFB400
                    .sub-title
                        color: rgba(#ffffff, .5)
                        margin: 15px 0
                        text-align: center
            #func-module
                padding: 25px
                overflow: hidden
                background-color: #ffffff
                .func-list
                    margin-top: 20px
                    display: block
                    .list-item
                        height: unset
                        box-shadow: none
                        border-radius: 10px
                        padding: 0
                        text-align: center
                        margin-right: 0
                        margin-bottom: 20px
                        .item-img
                            width: 68px
                            height: 68px
                            line-height: 68px
                            border-radius: 50%
                            background-color: #F2F2F2
                            margin: 0 auto
                            img
                                margin-top: 15px
                        .item-title
                            margin: 15px 0
                            font-size: 18px
                            color: #424345
                        .item-examples
                            color: #838383
            #economic-model
                background-color: #4C4592
                overflow: hidden
                padding: 25px
                .module-title
                    color: #ffffff
                .sub-title
                    color: rgba(#ffffff, .5)
                .model-list
                    margin-top: 40px
                    overflow: unset
                    .list-item
                        margin-bottom: 10px
                        width: calc(100% - 20px)
                        box-shadow: 0px 4px 26px 0px rgba(0,0,0,0.1)
                        padding: 10px
                        margin-right: 0
                        float: unset 
                        .title
                            color: #ffffff
                            font-size: 16px
                            margin-bottom: 10px
                            img
                                width: 32px
                                height: 32px
                                margin-right: 8px
                        p
                            color: rgba(#ffffff, .5)
                            line-height: 24px
            #team
                background-color: #ffffff
                overflow: hidden
                padding: 25px
                .team-list
                    margin-top: 20px
                    display: block
                    .list-item
                        margin-bottom: 10px
                        width: calc(100% - 20px)
                        padding: 10px
                        box-shadow: none
                        .item-left
                            float: unset
                            width: 118px
                            height: 118px
                            border-radius: 50%
                            margin: 0 auto 10px
                            img
                                border-radius: 50%
                        .item-right
                            float: unset
                            width: 100%
                            margin: 20px 0 0 0
                            .title
                                color: #424345
                                font-size: 24px
                                margin-bottom: 14px
                                span
                                    display: block
                                    font-size: 14px
                                    color: #A5A5A5
                                    margin-top: 5px
                            p
                                color: #838383
                                line-height: 22px
            #about
                background-color: #4C4592
                overflow: hidden
                padding: 25px
                .module-title
                    color: #ffffff
                .sub-title
                    color: rgba(#ffffff, .5)
                    margin-bottom: 20px
                img
                    max-width: 100%
            .module-title
                font-size: 24px
                color: #424345
                position: relative
                height: 60px
                line-height: 60px
                text-align: center
                &:after
                    content: ''
                    position: absolute
                    width: 26px
                    height: 3px
                    bottom: 0
                    left: 50%
                    margin-left: -13px
                    background-color: #FFB400
            .sub-title
                color: #838383
                margin: 25px 0
                text-align: center
        .footer
            height: 40px
            line-height: 40px
            padding: 0
            background-color: #ffffff
            text-align: center
            .f-left
                display: none
            .f-right
                float: unset
                font-size: 14px
                color: #838383

